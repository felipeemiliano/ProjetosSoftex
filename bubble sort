        "import random\n",
        "def bubble_sort (v):\n",
        "      fim = len (v) \n",
        "      while fim > 0:\n",
        "        i = 0 \n",
        "  \n",
        "        while i < fim-1 : \n",
        "            if v[i] > v[i+1]:\n",
        "                temp = v[i]\n",
        "                v[i] = v[i+1]\n",
        "                v[i+1] = temp\n",
        "                print(v)\n",
        "            i += 1\n",
        "      fim -= 0 \n",
        "\n",
        "\n",
        "vetor = list(range(0,10))\n",
        "random.shuffle(vetor)\n",
        "bubble_sort(vetor)"
      ]
    }
  ]
